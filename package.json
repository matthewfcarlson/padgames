{
  "name": "pad-games",
  "version": "0.0.3",
  "private": true,
  "author": {
    "name": "Matthew Carlson",
    "url": "https://matthewfcarlson.github.io"
  },
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/matthewfcarlson/padgames/issues"
  },
  "repository": "github:matthewfcarlson/padgames",
  "description": "Games based around vue, websockets, and bringing people together",
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^1.0.1",
    "@types/passport": "^1.0.1",
    "axios": "^0.19.0",
    "brain.js": "^1.6.1",
    "chart.js": "^2.8.0",
    "connect-history-api-fallback": "^1.6.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "javascript-state-machine": "^3.1.0",
    "mongoose": "^5.6.11",
    "passport": "^0.4.0",
    "random-js": "^1.0.8",
    "register-service-worker": "^1.6.2",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "underscore": "^1.9.1",
    "velocity-animate": "^1.5.2",
    "vue-axios": "^2.1.5",
    "vue-chartjs": "^3.4.2",
    "vue-class-component": "^7.0.2",
    "vue-directive-touch": "^1.0.28",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vue-scroll": "^2.1.12",
    "vuex": "^3.1.1"
  },
  "main": "dist_server/server/src/main.js",
  "scripts": {
    "e2e": "node test/e2e/runner.js",
    "test": "jest --config test/unit/jest.conf.js",
    "server-clean": "rimraf dist_server/*",
    "client-clean": "rimraf dist_client/*",
    "clean": "npm-run-all server-clean client-clean",
    "client-lint": "tslint -c tslint.json -p client",
    "client-lint-fix": "tslint -c tslint.json -p client --fix",
    "client-lint-prod": "tslint -c tslint.json -p client --strict",
    "server-lint": "tslint -c tslint.json -p server",
    "server-lint-fix": "tslint -c tslint.json -p server --fix",
    "server-lint-prod": "tslint -c tslint.json -p server --strict",
    "client-tsc": "webpack --config client/webpack.dev.js",
    "client-tsc-prod": "webpack --config client/webpack.prod.js",
    "server-tsc": "tsc -p server",
    "client-build": "npm-run-all client-lint client-tsc",
    "client-build-prod": "npm-run-all client-clean client-lint client-tsc-prod",
    "server-build": "npm-run-all server-lint server-tsc",
    "server-build-prod": "npm-run-all server-clean server-lint server-tsc",
    "server-dev": "nodemon --watch server -e ts,ejs,js --exec npm run server-build:start",
    "client-dev": "nodemon --watch client -e vue --exec npm run client-build",
    "server-build:dev": "npm-run-all server-build server-dev",
    "server-build:start": "npm-run-all server-build start",
    "client-build:dev": "npm-run-all client-build client-dev",
    "client": "npm run client-dev",
    "server": "npm run server-dev",
    "build-prod": "npm-run-all client-build-prod server-build-prod",
    "build": "npm-run-all  client-build server-build",
    "dev": "npm-run-all --parallel client-dev server-dev",
    "start": "node ."
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "devDependencies": {
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^23.1.4",
    "@types/node": "^12.7.2",
    "@types/shelljs": "^0.8.5",
    "@types/socket.io": "^2.1.2",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.6.1",
    "babel-core": "^6.26.3",
    "bootstrap": "^4.3.1",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons-webpack-plugin": "^1.0.1",
    "file-loader": "^4.2.0",
    "fs": "0.0.1-security",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "jest-css-modules": "^2.1.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jest-vue-preprocessor": "^1.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prerender-spa-plugin": "^3.4.0",
    "raw-loader": "^3.1.0",
    "rimraf": "^3.0.0",
    "sass": "^1.22.10",
    "sass-loader": "^8.0.0",
    "shelljs": "^0.8.3",
    "sitemap-webpack-plugin": "^0.6.0",
    "style-loader": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.1",
    "vue-socket.io": "^3.0.7",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vue2-filters": "^0.8.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-virtual-modules": "^0.1.11"
  }
}
