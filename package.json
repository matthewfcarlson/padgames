{
  "name": "padgames",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" --raw",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "dev:client": "vue-cli-service build --watch --mode development",
    "build:client": "vue-cli-service build --modern",
    "dev:server": "nodemon --config ./nodemon.config.json",
    "build:server": "rimraf dist/server && tsc -p tsconfig.server.json",
    "lint": "vue-cli-service lint",
    "start": "NODE_ENV=production node dist/server/server/server.js"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.6.0",
    "@vue/composition-api": "^1.1.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "rhino": "^5.3.7",
    "socket.io": "^4.5.1",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.176",
    "@types/three": "^0.131.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "^4.5.3",
    "@vue/cli-plugin-eslint": "^4.5.3",
    "@vue/cli-plugin-router": "^4.5.3",
    "@vue/cli-plugin-typescript": "^4.5.3",
    "@vue/cli-plugin-vuex": "~4.5.3",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "axios": "^0.21.3",
    "concurrently": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.0-beta.2",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "socket.io-client": "^4.2.0",
    "ts-node": "^8.8.2",
    "typescript": "^4.3.2",
    "vue-resource": "^1.5.3",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.6.2",
    "vuex-smart-module": "^1.0.0"
  }
}
